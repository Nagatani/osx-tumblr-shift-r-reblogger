{
  "manifest_version": 2,
  "name": "osx-tumblr-shift-r-reblogger",
  "description": "MacOS用 TumblrのキーボードショートカットでファストリブログをShift + Rでできるようにするやつ",
  "version": "0.0.2",
  "content_scripts": [
    {
      "matches": ["https://www.tumblr.com/dashboard/*"],
      "js": ["content.js"],
      "css": ["adblock.css"]
    }
  ],
  "web_accessible_resources": [
    "shiftRReblogger.js"
  ],
  "permissions": [
    "https://www.tumblr.com/"
  ]
}
